input-view {
  match: Result (this) {
    to-input: FindExit
  }

  message {
    template-macro (T-selectDestination)
  }

  render {
    selection-of (this) {
      where-each (one) {
        layout-macro (L-selectDestination) {
          param (result) {
            expression (one)
          }
        }
      }
    }
  }
}

