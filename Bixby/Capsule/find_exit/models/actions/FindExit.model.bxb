action (FindExit) {
  description (__DESCRIPTION__)
  collect {
    input (result) {
      type (Result)
      min (Required) max (One)
      prompt-behavior (AlwaysSelection)
      default-init {
        intent {
          goal: GetDestinationList
        }
      }
    }
  }
  type (Search)
  output (Description) {
    throws {
      unknown-error {
        on-catch {
          halt {
            dialog ("알 수 없는 에러가 발생하였습니다. 다시 시도해주시기 바랍니다.")
          }
        }
      }
    }
  }
}
