result-view {
  match: Result (this)

  message {
    choose (Random) {
      template ("런업의 #{value(this.index)}번째 영상입니다.")
      template ("#{value(this.title)}")
    }
  }

  render {
    layout {
      section {
        content {
          video {
            youtube {
              video-id ("#{value(this.token)}")
            }
          }
          attribution-link {
            label {
              template ("Open with browser or YouTube")
            }
            url {
              template ("#{value(this.url)}")
            }
          }
        }
      }
    }
  }
}